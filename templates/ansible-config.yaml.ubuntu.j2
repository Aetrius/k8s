
# This is the network config written by 'ansible'
network:
  ethernets:
    enp0s18:
      dhcp4: false
      addresses:
        - {{ansible_host}}/24
      nameservers:
        addresses: [8.8.8.8, 8.8.6.6] # Populate this list with the DNS servers necessary. 
      routes: 
        - to: default
          via: 192.168.1.1
      renderer: networkd
      dhcp-identifier: mac
  version: 2